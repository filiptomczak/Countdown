<Window x:Class="Minutnik.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Minutnik"
        mc:Ignorable="d"
        Title="Counter" Height="400.0" Width="340.0" MinWidth="340" MinHeight="400" MaxWidth="340" MaxHeight="400">
    <Grid Margin="0,0,0,3" HorizontalAlignment="Left" Width="330">
        <Grid.ColumnDefinitions>
            <ColumnDefinition MinWidth="1"/>
            <ColumnDefinition Width="0*"/>
        </Grid.ColumnDefinitions>
        <Button x:Name="button_set" Content="Set" HorizontalAlignment="Center" Margin="0,0,0,10" VerticalAlignment="Bottom"
                Width="300" RenderTransformOrigin="-0.792,0.392" Height="30" FontFamily="GENISO" FontSize="16" FontWeight="Bold"
                BorderThickness="0" Background="#FF945EC1" Click="button_set_Click" MouseEnter="button_mouseEnter" />
        <local:Zegar x:Name="zegar" Margin="0,10,0,0" VerticalAlignment="Top"></local:Zegar>
    </Grid>
</Window>
